<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- mpeg2_dec.vcxproj specific settings -->
  <PropertyGroup Label="mpeg2_dec settings">
    <MfxUsingSettings>mpeg2_dec;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\mpeg2_dec\sw\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\io\umc_va\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    <!-- libmfxsw -->
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\mjpeg\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\h264\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\vp9\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\io\media_buffers\include;
    </MfxIncludes>
    <MfxDefinitions>MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxDefinitions></MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
  </PropertyGroup>
<!-- End of mpeg2_dec.vcxproj specific settings -->
</Project>
