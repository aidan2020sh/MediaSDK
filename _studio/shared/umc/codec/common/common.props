<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- common.vcxproj specific settings -->
  <PropertyGroup Label="common settings">
    <MfxUsingSettings>common;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppGoldIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\common\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\aac_common\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\mp3_common\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    </MfxIncludes>
    <MfxDefinitions>MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
    <MfxTreatWarningAsError>false</MfxTreatWarningAsError>
  </PropertyGroup>
<!-- End of common.vcxproj specific settings -->
</Project>
