<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- asclib.vcxproj specific settings -->
  <PropertyGroup Label="asclib settings">
    <MfxUsingSettings>ascLib;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\asc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\cmrt_cross_platform\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    </MfxIncludes>
    <MfxDefinitions>CM_DX11;MFX_VA;MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
    <MfxEnableEnhancedInstructionSet>NotSet</MfxEnableEnhancedInstructionSet>
    <MfxFloatingPointExceptions>false</MfxFloatingPointExceptions>
    <MfxFavorSizeOrSpeed>Speed</MfxFavorSizeOrSpeed>
    <MfxIntrinsicFunctions>true</MfxIntrinsicFunctions>
    <MfxInterproceduralOptimization>NoIPO</MfxInterproceduralOptimization>
  </PropertyGroup>
<!-- End of asclib.vcxproj specific settings -->
</Project>
