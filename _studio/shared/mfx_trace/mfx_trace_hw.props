<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- mfx_trace_hw.vcxproj specific settings -->
  <PropertyGroup Label="mfx_trace_hw settings">
    <MfxUsingSettings>mfx_trace_hw;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_API_ROOT)\mediasdk_structures\;
    $(MSDK_STUDIO_ROOT)\shared\mfx_trace\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MfxRootOutDir)lib;
    </MfxIncludes>
    <MfxDefinitions>MFX_VA;MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
    <MfxPreBuildEvent>mc -h $(MfxRootOutDir)lib -r $(MfxRootOutDir)lib -z media_sdk_etw -um media_sdk_etw.man</MfxPreBuildEvent>
    <MfxPreBuildEventMessage>Running $(MfxPreBuildEvent) to compile MediaSDK ETW manifest</MfxPreBuildEventMessage>
  </PropertyGroup>
<!-- End of mfx_trace_hw.vcxproj specific settings -->
</Project>
