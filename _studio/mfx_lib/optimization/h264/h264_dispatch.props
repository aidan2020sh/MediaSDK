<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- h264_dispatch.vcxproj specific settings -->
  <PropertyGroup Label="h264_dispatch settings">
    <MfxUsingSettings>h264_dispatch;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\optimization\h265\include;
    <!-- libmfxsw -->
    $(MSDK_STUDIO_ROOT)\mfx_lib\optimization\h264\include;
    </MfxIncludes>
    <MfxDefinitions>AS_HEVCD_PLUGIN;AS_HEVCE_PLUGIN;MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxDefinitions Condition="'$(MfxSoftwareOnly)'!='true'">MFX_VA;$(MfxDefinitions)</MfxDefinitions>
    <MfxDefinitions Condition="'$(Configuration)'=='Debug_HSW_Emu'">CMRT_EMU;$(MfxDefinitions)</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
  </PropertyGroup>
<!-- End of h264_dispatch.vcxproj specific settings -->
</Project>
