<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- encode.vcxproj specific settings -->
  <PropertyGroup Label="encode settings">
    <MfxUsingSettings>encode;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\mpeg2_enc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\mjpeg_enc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\h264_enc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\brc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\jpeg_enc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\color_space_converter\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\encode\mjpeg\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\encode\mpeg2\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\encode\h264\include;
    </MfxIncludes>
    <MfxDefinitions>MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
  </PropertyGroup>
<!-- End of encode.vcxproj specific settings -->
</Project>
