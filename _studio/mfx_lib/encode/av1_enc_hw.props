<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- h265_enc_hw.vcxproj specific settings -->
  <PropertyGroup Label="av1_enc_hw settings">
    <MfxUsingSettings>av1_enc_hw;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\fs_det\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\cmrt_cross_platform\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\encode\av1\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\fei\av1\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\optimization\av1\include;
    </MfxIncludes>
    <MfxDefinitions>AS_AV1E_PLUGIN;MFX_D3D11_ENABLED;MFX_VA;UNICODE;</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
    <MfxPlatformToolset>$(MfxIntelPlatformToolset)</MfxPlatformToolset>
  </PropertyGroup>
<!-- End of h265_enc_hw.vcxproj specific settings -->
</Project>
