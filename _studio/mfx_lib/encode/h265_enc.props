<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- h265_enc.vcxproj specific settings -->
  <PropertyGroup Label="h265_enc settings">
    <MfxUsingSettings>h265_enc;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\fs_det\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\cmrt_cross_platform\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\encode\h265\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\fei\h265\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\optimization\h265\include;
    </MfxIncludes>
    <MfxDefinitions>AS_HEVCE_PLUGIN;MFX_D3D11_ENABLED;UNICODE;</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
    <MfxPlatformToolset>$(MfxIntelPlatformToolset)</MfxPlatformToolset>
  </PropertyGroup>
<!-- End of h265_enc.vcxproj specific settings -->
</Project>
