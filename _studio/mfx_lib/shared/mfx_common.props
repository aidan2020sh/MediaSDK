<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- mfx_common.vcxproj specific settings -->
  <PropertyGroup Label="mfx_common settings">
    <MfxUsingSettings>mfx_common;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\brc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\mpeg2_dec\sw\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\mpeg2_dec\hw\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\h264_dec\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\vc1_common\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\h264_enc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\vc1_dec\include;
    </MfxIncludes>
    <MfxDefinitions>MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxDefinitions Condition="'$(MfxSoftwareOnly)'!='true'">MFX_VA;$(MfxDefinitions)</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
  </PropertyGroup>
<!-- End of mfx_common.vcxproj specific settings -->
</Project>
