<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- decode.vcxproj specific settings -->
  <PropertyGroup Label="decode settings">
    <MfxUsingSettings>decode;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
    $(MfxIppIncludeDir);
    $(MSDK_API_ROOT)\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\h264_dec\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\vp9_dec\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\jpeg_dec\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\mpeg2_dec\sw\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\vc1_dec\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\vc1_common\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\codec\jpeg_common\include;
    $(MSDK_STUDIO_ROOT)\shared\umc\io\media_buffers\include;
    $(MSDK_STUDIO_ROOT)\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\mjpeg\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\h264\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\vc1\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\vp9\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\decode\mpeg2\sw\include;
    $(MSDK_STUDIO_ROOT)\mfx_lib\lpla;
    </MfxIncludes>
    <MfxDefinitions>MFX_D3D11_ENABLED;</MfxDefinitions>
    <MfxLibraries></MfxLibraries>
    <MfxLibraryDir></MfxLibraryDir>
  </PropertyGroup>
<!-- End of decode.vcxproj specific settings -->
</Project>
