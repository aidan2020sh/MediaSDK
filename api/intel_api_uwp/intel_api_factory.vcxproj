  <PropertyGroup>
    <MfxAppxPacking>set tmp_path=$(MfxBuildDir)win_$(PlatformName)\Intel.MediaSDK.$(PlatformTarget)
mkdir "%tmp_path%"
copy "$(OutDir)$(TargetName).dll" "%tmp_path%"
copy "$(MSBuildProjectDirectory)\dfp\AppxManifest.$(PlatformTarget).xml" "%tmp_path%\AppxManifest.xml"
copy "$(MSBuildProjectDirectory)\dfp\logo.png" "%tmp_path%"
del /q "$(OutDir)Intel.MediaSDK.$(PlatformTarget).appx"
makeappx.exe pack /p "$(OutDir)Intel.MediaSDK.$(PlatformTarget).appx" /d "%tmp_path%" /l /h sha256
signtool sign /fd sha256 /f "$(MSBuildProjectDirectory)\dfp\key.pfx" "$(OutDir)Intel.MediaSDK.$(PlatformTarget).appx"
rmdir /s /q "%tmp_path%"</MfxAppxPacking>
