    <MfxDefinitions>GTEST_HAS_PTHREAD=0</MfxDefinitions>
    <MfxDisableSpecificWarnings>4251;4275;4702</MfxDisableSpecificWarnings>
  </PropertyGroup>
<!-- End of compare_struct.vcxproj specific settings -->
