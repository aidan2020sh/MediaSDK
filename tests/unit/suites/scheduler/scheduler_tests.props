<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- scheduler_tests.vcxproj specific settings -->
  <PropertyGroup Label="scheduler_tests settings">
    <MfxConfigurationType Label="scheduler_tests settings">Application</MfxConfigurationType>
    <MfxUsingSettings>scheduler_tests;$(MfxUsedSettings)</MfxUsingSettings>
    <MfxIncludes>
      $(MSDK_API_ROOT)\include;
      $(MSDK_STUDIO_ROOT)\shared\include;
      $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
      $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
      $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
      $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
      $(MSDK_STUDIO_ROOT)\mfx_lib\scheduler\include;
      $(MSDK_STUDIO_ROOT)\mfx_lib\scheduler\windows\include;
      $(MSDK_STUDIO_ROOT)\..\contrib\googletest\googletest\include;
      $(MSDK_STUDIO_ROOT)\..\contrib\googletest\googlemock\include;
      $(MfxIppIncludeDir)
    </MfxIncludes>
    <MfxTreatWarningAsError>false</MfxTreatWarningAsError>
    <MfxTargetName>$(ProjectName)</MfxTargetName>
    <!-- IPP libraries -->
    <MfxLibraryDir>$(MfxIppLibraryDir)</MfxLibraryDir>
    <MfxLibraries Condition="'$(Platform)'=='Win32'">ippcoremt.lib;ippi_s_p8.lib;$(MfxLibraries)</MfxLibraries>
    <MfxLibraries Condition="'$(Platform)'=='x64'">ippcoremt.lib;ippi_s_y8.lib;$(MfxLibraries)</MfxLibraries>
  </PropertyGroup>
<!-- End of scheduler_tests.vcxproj specific settings -->
</Project>
