<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
<!-- av1e.vcxproj specific settings -->
  <PropertyGroup Label="av1e settings">
    <MfxConfigurationType Label="av1e settings">Application</MfxConfigurationType>
    <MfxIncludes>
      $(MSDK_API_ROOT)\include;
      $(MSDK_STUDIO_ROOT)\shared\include;
      $(MSDK_STUDIO_ROOT)\shared\umc\core\umc\include;
      $(MSDK_STUDIO_ROOT)\shared\umc\core\vm\include;
      $(MSDK_STUDIO_ROOT)\shared\umc\core\vm_plus\include;
      $(MSDK_STUDIO_ROOT)\mfx_lib\shared\include;
      $(MSDK_STUDIO_ROOT)\mfx_lib\encode_hw\av1\agnostic;
      $(MSDK_STUDIO_ROOT)\mfx_lib\encode_hw\av1\agnostic\g12;
      $(MSDK_STUDIO_ROOT)\mfx_lib\encode_hw\av1\embargo\windows;
      $(MSDK_STUDIO_ROOT)\mfx_lib\encode_hw\av1\embargo\windows\g12;
      $(MSDK_STUDIO_ROOT)\mfx_lib\encode_hw\shared;
      $(MSDK_STUDIO_ROOT)\..\tests\unit;
      $(MSDK_STUDIO_ROOT)\..\contrib\detours\include;
      $(MSDK_STUDIO_ROOT)\..\contrib\googletest\googletest\include;
      $(MSDK_STUDIO_ROOT)\..\contrib\googletest\googlemock\include;
      $(MfxIppIncludeDir)
    </MfxIncludes>
    <MfxTreatWarningAsError>false</MfxTreatWarningAsError>
    <MfxTargetName>$(ProjectName)</MfxTargetName>
    <MfxDefinitions>MFX_VA;MFX_D3D11_ENABLED;$(MfxDefinitions);</MfxDefinitions>
    <MfxDefinitions Condition="'$(Platform)'=='x64'">MFX_ENABLE_UNIT_TEST_AV1E_DX11_DDI;$(MfxDefinitions);</MfxDefinitions>
    <MfxLibraryDir>$(MfxIppLibraryDir)</MfxLibraryDir>
    <!-- System DX API libraries -->
    <MfxLibraries>uuid.lib;dxva2.lib;d3d9.lib;d3d11.lib;dxgi.lib</MfxLibraries>
    <!-- IPP libraries -->
    <MfxLibraries Condition="'$(Platform)'=='Win32'">ippcoremt.lib;ippi_s_p8.lib;$(MfxLibraries)</MfxLibraries>
    <MfxLibraries Condition="'$(Platform)'=='x64'">ippcoremt.lib;ippi_s_y8.lib;$(MfxLibraries)</MfxLibraries>
    <!-- Detours library -->
    <MfxLibraries Condition="'$(Platform)'=='Win32'">$(MSDK_STUDIO_ROOT)\..\contrib\detours\lib.X86\detours.lib;$(MfxLibraries)</MfxLibraries>
    <MfxLibraries Condition="'$(Platform)'=='x64'">$(MSDK_STUDIO_ROOT)\..\contrib\detours\lib.X64\detours.lib;$(MfxLibraries)</MfxLibraries>
  </PropertyGroup>
<!-- End of av1e.vcxproj specific settings -->
</Project>
